{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      courseList: {\n        \"spring\": [\"Beijing\", \"Shanghai\", \"Guangzhou\", \"Tianjin\"],\n        \"fall\": [\"Beijing1\", \"Shanghai2\", \"Guangzhou3\", \"Tianjin4\"],\n        \"summer\": [\"Beijing2\", \"Shanghai1\", \"ssasd\", \"asd\"]\n      },\n      firstName: '',\n      lastName: '',\n      email: '',\n      semester: '',\n      course: '',\n      note: '',\n      returned: '',\n      courses: [],\n      errors: [],\n      successMessage: '',\n      user: {\n        firstName: this.firstName,\n        lastName: this.lastName,\n        email: this.email,\n        semester: this.semester,\n        course: this.course,\n        note: this.note\n      }\n    };\n  },\n\n  methods: {\n    handleSubmit() {\n      /* let info = [\n         this.firstName,\n         this.lastName,\n         this.email,\n         this.semester,\n         this.course,\n         this.note\n       ];*/\n      this.user.firstName = this.firstName;\n      this.user.lastName = this.lastName;\n      this.user.email = this.email;\n      this.user.semester = this.semester;\n      this.user.course = this.course;\n      this.user.note = this.note;\n      this.errors = [];\n      this.successMessage = '';\n      axios.post(\"http://localhost:3000/api/sendRegistration\", this.user).then(res => {\n        this.errors = [];\n        this.successMessage = \"Success, Form submitted!\";\n        console.log(res);\n      }).catch(error => {\n        if (error.response) {\n          // The request was made and the server responded with a status code that falls out of the range of 2xx\n          const propertyValues = Object.values(error.response.data);\n          this.errors = propertyValues;\n          console.log(error.response.data);\n        } else if (error.request) {\n          console.log(error.request); // The request was made but no response was received\n        } else {\n          console.log('Error', error.message); // Something happened in setting up the request that triggered an Error\n        }\n      });\n      /*     fetch(\"/api/sendRegistration?personForm=\" + info)\n               .then(response => response.text())\n               //.then(data =>{ this.returned = data })*/\n    }\n\n  },\n  watch: {\n    semester(selected) {\n      // clear previously selected values\n      this.course = '';\n      this.courses = []; //set courses depending on the selected semester\n\n      this.courses = this.courseList[selected];\n    }\n\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAuDA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACbC,IAAI,GAAG;IAEL,OAAO;MACLC,UAAU,EAAE;QACV,UAAU,CAAC,SAAD,EAAY,UAAZ,EAAwB,WAAxB,EAAqC,SAArC,CADA;QAEV,QAAQ,CAAC,UAAD,EAAa,WAAb,EAA0B,YAA1B,EAAwC,UAAxC,CAFE;QAGV,UAAU,CAAC,UAAD,EAAa,WAAb,EAA0B,OAA1B,EAAmC,KAAnC;MAHA,CADP;MAMLC,SAAS,EAAE,EANN;MAOLC,QAAQ,EAAC,EAPJ;MAQLC,KAAK,EAAE,EARF;MASLC,QAAQ,EAAE,EATL;MAULC,MAAM,EAAE,EAVH;MAWLC,IAAI,EAAE,EAXD;MAYLC,QAAQ,EAAE,EAZL;MAaLC,OAAO,EAAE,EAbJ;MAcLC,MAAK,EAAI,EAdJ;MAeLC,cAAa,EAAI,EAfZ;MAgBLC,IAAI,EAAE;QACJV,SAAS,EAAE,KAAKA,SADZ;QAEJC,QAAQ,EAAE,KAAKA,QAFX;QAGJC,KAAK,EAAE,KAAKA,KAHR;QAIJC,QAAQ,EAAE,KAAKA,QAJX;QAKJC,MAAM,EAAE,KAAKA,MALT;QAMJC,IAAI,EAAE,KAAKA;MANP;IAhBD,CAAP;EA0BD,CA7BY;;EA8BbM,OAAO,EAAE;IACPC,YAAY,GAAG;MACd;;;;;;;;MASC,KAAKF,IAAL,CAAUV,SAAV,GAAqB,KAAKA,SAA1B;MACA,KAAKU,IAAL,CAAUT,QAAV,GAAoB,KAAKA,QAAzB;MACA,KAAKS,IAAL,CAAUR,KAAV,GAAiB,KAAKA,KAAtB;MACA,KAAKQ,IAAL,CAAUP,QAAV,GAAoB,KAAKA,QAAzB;MACA,KAAKO,IAAL,CAAUN,MAAV,GAAkB,KAAKA,MAAvB;MACA,KAAKM,IAAL,CAAUL,IAAV,GAAgB,KAAKA,IAArB;MAEA,KAAKG,MAAL,GAAY,EAAZ;MACA,KAAKC,cAAL,GAAoB,EAApB;MAEAZ,KAAK,CAACgB,IAAN,CAAW,4CAAX,EAAyD,KAAKH,IAA9D,EACKI,IADL,CACUC,GAAE,IAAK;QACX,KAAKP,MAAL,GAAa,EAAb;QACA,KAAKC,cAAL,GAAqB,0BAArB;QACAO,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACD,CALL,EAMKG,KANL,CAMWC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACC,QAAV,EAAoB;UAClB;UACA,MAAMC,cAAa,GAAIC,MAAM,CAACC,MAAP,CAAcJ,KAAK,CAACC,QAAN,CAAetB,IAA7B,CAAvB;UAEA,KAAKU,MAAL,GA