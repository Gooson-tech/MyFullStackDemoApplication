ange2(event) {\n\n      this.firstName=event.target.value;\n\n\n    }\n  },\n  watch: {\n\n    semester(newQuestion) {\n      if (newQuestion === \"spring\") {\n        // Clear previously selected values\n        this.CurrentCourseList = [];\n        // Populate list of countries in the second dropdown\n        this.CurrentCourseList = this.courseList[newQuestion];\n\n      }\n      else {\n        this.note = newQuestion;\n      }\n    }\n\n  },\n\n  mounted() {\n\n   /* fetch(\"/api/messages/greeting\")\n        .then((response)=> response.text())\n        .then((data)=>{ this.greeting=data })*/\n\n\n  }\n}\n\n\n\n\n</script>\n\n\n<style scoped>\n\nform {\n  max-width: 600px;\n  margin: 30px auto;\n  background: #fff;\n  text-align: left;\n  padding: 20px;\n  border-radius: 10px;\n}\n\nlabel {\n  color: #aaa;\n  display: inline-block;\n  margin: 25px 0 15px;\n  text-transform: uppercase;\n}\n\ninput, select {\n  display: block;\n  padding: 10px 6px;\n  width: 100%;\n  box-sizing: border-box;\n  border: none;\n  border-bottom: 1px solid #ddd;\n  color: #555;\n}\n\nbutton {\n  background: rgb(7, 24, 7);\n  border: 0;\n  padding: 10px 20px;\n  color: white;\n  border-radius: 20px;\n}\n\ntextarea {\n  outline: #2c2c2c;\n  padding: 10px 6px;\n  width: 100%;\n  box-sizing: border-box;\n  color: #555;\n  resize: none;\n}\n</style>"]},"metadata":{},"sourceType":"module"}