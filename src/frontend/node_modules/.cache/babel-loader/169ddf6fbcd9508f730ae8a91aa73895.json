{"ast":null,"code":"export default {\n  data() {\n    return {\n      courseList: {\n        \"spring\": [\"Beijing\", \"Shanghai\", \"Guangzhou\", \"Tianjin\"],\n        \"fall\": [\"Beijing1\", \"Shanghai2\", \"Guangzhou3\", \"Tianjin4\"],\n        \"summer\": [\"Beijing2\", \"Shanghai1\", \"ssasd\", \"asd\"]\n      },\n      firstName: '',\n      lastName: '',\n      email: '',\n      semester: '',\n      course: '',\n      textArea: '',\n      note: '',\n      returned: '',\n      courses: []\n    };\n  },\n\n  methods: {\n    handleSubmit() {\n      let info = [this.firstName, this.lastName, this.email, this.semester, this.course, this.note];\n      fetch(\"/api/sendRegistration?info=\" + info).then(response => response.text()).then(data => {\n        this.returned = data;\n      });\n    }\n\n  },\n  watch: {\n    semester(semest) {\n      // Clear previously selected values\n      this.course = '';\n      this.courses = []; //set courses depending on the selected semester\n\n      this.courses = this.courseList[semest];\n    }\n\n  },\n\n  mounted() {}\n\n};","map":{"version":3,"mappings":"AAiDA,eAAe;EACbA,IAAI,GAAG;IAEL,OAAO;MACLC,UAAU,EAAE;QACV,UAAU,CAAC,SAAD,EAAY,UAAZ,EAAwB,WAAxB,EAAqC,SAArC,CADA;QAEV,QAAQ,CAAC,UAAD,EAAa,WAAb,EAA0B,YAA1B,EAAwC,UAAxC,CAFE;QAGV,UAAU,CAAC,UAAD,EAAa,WAAb,EAA0B,OAA1B,EAAmC,KAAnC;MAHA,CADP;MAMLC,SAAS,EAAE,EANN;MAOLC,QAAQ,EAAC,EAPJ;MAQLC,KAAK,EAAE,EARF;MASLC,QAAQ,EAAE,EATL;MAULC,MAAM,EAAE,EAVH;MAWLC,QAAQ,EAAE,EAXL;MAYLC,IAAI,EAAE,EAZD;MAaLC,QAAQ,EAAE,EAbL;MAcLC,OAAO,EAAE;IAdJ,CAAP;EAgBD,CAnBY;;EAoBbC,OAAO,EAAE;IACPC,YAAY,GAAG;MACX,IAAIC,IAAG,GAAI,CACT,KAAKX,SADI,EAET,KAAKC,QAFI,EAGT,KAAKC,KAHI,EAIT,KAAKC,QAJI,EAKT,KAAKC,MALI,EAMT,KAAKE,IANI,CAAX;MAQAM,KAAK,CAAC,gCAA+BD,IAAhC,CAAL,CACKE,IADL,CACUC,QAAO,IAAKA,QAAQ,CAACC,IAAT,EADtB,EAEKF,IAFL,CAEUf,IAAG,IAAI;QAAE,KAAKS,QAAL,GAAgBT,IAAhB;MAAsB,CAFzC;IAGH;;EAbM,CApBI;EAoCbkB,KAAK,EAAE;IACLb,QAAQ,CAACc,MAAD,EAAS;MACf;MACA,KAAKb,MAAL,GAAc,EAAd;MACA,KAAKI,OAAL,GAAe,EAAf,CAHe,CAIf;;MACA,K